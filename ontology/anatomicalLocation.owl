    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#abaxial</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Awy from the organisms central axis: x.abaxial y means y is more towards the outside of the body, for example spine.abaxial lungs.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>#abaxial</IRI>
        <Literal datatypeIRI="&xsd;anyURI">img/axial-skull_interior_anatomy.svg</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#anterior</IRI>
        <Literal datatypeIRI="&xsd;string">The front-end: x.anterior y means that y is more towards the nose than x</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>#anterior</IRI>
        <Literal datatypeIRI="&xsd;anyURI">img/anteroposterior-head_anatomy_lateral_view.svg</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#axial</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the central axis of the organism. x.axial y means that y is more towards the central axis than x: lung.axial spinalcord</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>#axial</IRI>
        <Literal datatypeIRI="&xsd;anyURI">img/axial-skull_interior_anatomy.svg</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#caudal</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the feet</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#contralateral</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">On the opposite side</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#distal</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the end of an appendage; x.distal y means that y is more towards the end of the appendage than x e.g. elbow.distal hand</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#dorsal</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">x.dorsal y means y is more towards the back than x at least when x and y are part of the torso, when x and y are part of the head then x.dorsal y means y is more towards the top of the head</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#ipsilateral</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">On the same side of the body</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#lateral</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the side. x.lateral y means y is more towards the side than x.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#left</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">x.left y means y is more towards the left-side than x</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#median</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the center; x.medial y; means y is more towards the center of the body than x.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#parietal</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">A &#39;wall&#39; of a body cavity.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#posterior</IRI>
        <Literal datatypeIRI="&xsd;string">To the back: eyes.posterior hypophysis means the hypophysis is more towards the back of the head than the eyes.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:seeAlso"/>
        <IRI>#posterior</IRI>
        <Literal datatypeIRI="&xsd;anyURI">img/anteroposterior-head_anatomy_lateral_view.svg</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#proximal</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards where the appendage is attached</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#rostral</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the nose</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#superficial</IRI>
        <Literal datatypeIRI="&rdf;PlainLiteral">Towards the outer surface; x.superfical y mean y is closer to the body&#39;s surface than x</Literal>
